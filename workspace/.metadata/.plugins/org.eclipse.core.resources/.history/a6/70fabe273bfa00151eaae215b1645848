package rover_positioning;

//test class


class MissionSpec{
	
	//Input and Output
	//Test Input:
	 private int number_of_missions;
	 private int number_of_rovers;
	 private String plateau;//dimensions of the mission space
	 
	 
	 String rover_1_start ="1 2 N";
	 String rover_1_move_order = "LMLMLMLMM";
	
	 String rover_2_start ="3 3 E";
	 String rover_2_move_order ="MMRMMRMRRM";
	//Expected Output:
	 String rover_1_end = "1 3 N";
	 String rover_2_end = "5 1 E";
	 
	 public void setNumberofMissions(int numberOfMissions) {
		number_of_missions = numberOfMissions;
		
		}
	 public void setNumberofRovers(int numberOfRovers) {
		 number_of_rovers = numberOfRovers;
	}
	 
	public int getNumberOfMissions(){
		 return number_of_missions;
	 }
	public int getNumberOfRovers(){
		return number_of_rovers;
	}
	
	public void setMissionSpace(String a){
		plateau = a;
	}
	
	public String getMissionSpace(){
		return plateau;
	}
	
	 //constructor
	public MissionSpec(){
	
	}
	

	 //constructor
}
//test class